<?xml version="1.0" encoding="utf-8"?>
<AnimData xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="animationdata.xsd">
  <!-- 'instant' overrides any set blendSpeeds -->
    <!-- '6.6666' was the default blend-speed value since 2015 and is also used when blendSpeed here is set to zero or not set at all -->
    <!-- LayberBlendInfos written in ALL CAPS are automatically assigned (i.e. to every MOB) -->
  <LayerBlendInfos>
    <LayerBlendInfo name="MOB_DEFAULT_BLENDINFO">
      <default>
        <in instant="true"/>
        <out blendSpeed ="0"/>
      </default>
    </LayerBlendInfo>
    <LayerBlendInfo name="Instant_In_Out">
      <default>
        <in instant="true" />
        <out instant="true" />
      </default>
    </LayerBlendInfo>   
    <LayerBlendInfo name="Instant_In_Default_Ease_Out">
      <default>
        <in instant="true"/>
        <out blendSpeed ="0"/>
      </default>
    </LayerBlendInfo>
  </LayerBlendInfos>
  
</AnimData>