<?xml version="1.0" encoding="utf-8"?>
<patch xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="patch.xsd">
	
	<!--GVG Season Dates-->
	<!--Season 20-->
	<replace sel="Times/DateTime[@uniquename='GVG_SEASON_20_END']">
		<attribute name="datetime" newvalue="2023-09-11T01:00:00Z"/>
	</replace>
	<replace sel="Times/DateTime[@uniquename='GVG_SEASON_20_START']">
		<attribute name="datetime" newvalue="2023-06-17T01:00:00Z"/>
	</replace>
	<!--Season 19-->
	<replace sel="Times/DateTime[@uniquename='GVG_SEASON_19_END']">
		<attribute name="datetime" newvalue="2023-06-05T01:00:00Z"/>
	</replace>
	<replace sel="Times/DateTime[@uniquename='GVG_SEASON_19_START']">
		<attribute name="datetime" newvalue="2023-03-25T01:00:00Z"/>
	</replace>
	<!--Season 18-->
	<replace sel="Times/DateTime[@uniquename='GVG_SEASON_18_END']">
		<attribute name="datetime" newvalue="2023-03-13T01:00:00Z"/>
	</replace>
	<replace sel="Times/DateTime[@uniquename='GVG_SEASON_18_START']">
		<attribute name="datetime" newvalue="2023-02-20T01:00:00Z"/>
	</replace>
	
	<!-- Downtime  -->
	<replace sel="Times/Time[@uniquename='DOWNTIME']">
		<attribute name="time" newvalue="00:00:00"/>
	</replace>	
	
	<!-- events -->
	<replace sel="Times/DateTime[@uniquename='EVENT_EASTER_2022_START']">
		<attribute name="datetime" newvalue="2022-04-02T00:00:00Z"/>
	</replace>
	<replace sel="Times/DateTime[@uniquename='EVENT_EASTER_2022_END']">
		<attribute name="datetime" newvalue="2022-04-26T00:00:00Z"/>
	</replace>
	<replace sel="Times/DateTime[@uniquename='EVENT_ANNIVERSARY_2022_START']">
		<attribute name="datetime" newvalue="2022-07-14T00:00:00Z"/>
	</replace>
	<replace sel="Times/DateTime[@uniquename='EVENT_ANNIVERSARY_2022_END']">
		<attribute name="datetime" newvalue="2022-07-28T00:00:00Z"/>
	</replace>
	<replace sel="Times/DateTime[@uniquename='EVENT_HALLOWEEN_2022_START']">
		<attribute name="datetime" newvalue="2022-10-27T00:00:00Z"/>
	</replace>
	<replace sel="Times/DateTime[@uniquename='EVENT_HALLOWEEN_2022_END']">
		<attribute name="datetime" newvalue="2022-11-10T00:00:00Z"/>
	</replace>
  <replace sel="Times/DateTime[@uniquename='EVENT_XMAS_2022_START']">
    <attribute name="datetime" newvalue="2022-12-16T00:00:00Z"/>
  </replace>
  <replace sel="Times/DateTime[@uniquename='EVENT_XMAS_2022_END']">
    <attribute name="datetime" newvalue="2022-12-30T00:00:00Z"/>
  </replace>

	<!-- REFERRAL -->
	<!-- Hide Skin Until (season start) -->
	<replace sel="Times/DateTime[@uniquename='REFERRAL_2022_OCT_HIDE']">
		<attribute name="datetime" newvalue="2022-10-01T00:00:00Z"/>
	</replace>
	<replace sel="Times/DateTime[@uniquename='REFERRAL_2022_DEC_HIDE']">
		<attribute name="datetime" newvalue="2022-12-01T00:00:00Z"/>
	</replace>
	<replace sel="Times/DateTime[@uniquename='REFERRAL_2023_FEB_HIDE']">
		<attribute name="datetime" newvalue="2023-02-01T00:00:00Z"/>
	</replace>

	<!-- Block Skin Trade Until (2 month + 15 days after season ends) -->
	<replace sel="Times/DateTime[@uniquename='REFERRAL_2022_OCT_TRADE']">
		<attribute name="datetime" newvalue="2022-12-15T00:00:00Z"/>
	</replace>
	<replace sel="Times/DateTime[@uniquename='REFERRAL_2022_DEC_TRADE']">
		<attribute name="datetime" newvalue="2022-02-15T00:00:00Z"/>
	</replace>
	<replace sel="Times/DateTime[@uniquename='REFERRAL_2023_FEB_TRADE']">
		<attribute name="datetime" newvalue="2023-04-15T00:00:00Z"/>
	</replace>

</patch>
